<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>BookRegisterServlet</servlet-name>
  	<display-name>BookRegisterServlet</display-name>
  	<description></description>
  	<servlet-class>tasks.BookRegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DisplayBooksServlet</servlet-name>
  	<display-name>DisplayBooksServlet</display-name>
  	<description></description>
  	<servlet-class>tasks.DisplayBooksServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteBookServlet</servlet-name>
  	<display-name>DeleteBookServlet</display-name>
  	<description></description>
  	<servlet-class>tasks.DeleteBookServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SearchBooksServlet</servlet-name>
  	<display-name>SearchBooksServlet</display-name>
  	<description></description>
  	<servlet-class>tasks.SearchBooksServlet</servlet-class>
  </servlet>
